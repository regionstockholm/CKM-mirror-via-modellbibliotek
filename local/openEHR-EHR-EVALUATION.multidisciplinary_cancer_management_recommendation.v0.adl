definition
	EVALUATION[at0000] matches {    -- multidisciplinary cancer management recommendation
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					CLUSTER[at0002] occurrences matches {0..1} matches {    -- Recommended treatment strategy
						items cardinality matches {1..*; unordered} matches {
							CLUSTER[at0004] occurrences matches {0..1} matches {    -- Treatment option
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0005] occurrences matches {0..1} matches {    -- Clinical study
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0006] occurrences matches {0..1} matches {    -- Option number
										value matches {
											DV_COUNT matches {*}
										}
									}
									CLUSTER[at0007] occurrences matches {0..1} matches {    -- Main treatment
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0010] occurrences matches {0..1} matches {    -- Intent
												value matches {
													DV_CODED_TEXT matches {*}
												}
											}
											ELEMENT[at0011] occurrences matches {0..1} matches {    -- Type of treatment
												value matches {
													DV_CODED_TEXT matches {*}
												}
											}
											allow_archetype CLUSTER[at0014] occurrences matches {0..*} matches {    -- Additional details
												include
													archetype_id/value matches {/.*/}
											}
										}
									}
									CLUSTER[at0008] occurrences matches {0..1} matches {    -- Additional treatment
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0015] occurrences matches {0..1} matches {    -- Intent
												value matches {
													DV_CODED_TEXT matches {*}
												}
											}
											ELEMENT[at0016] occurrences matches {0..1} matches {    -- Type of treatment
												value matches {
													DV_CODED_TEXT matches {*}
												}
											}
											ELEMENT[at0028] occurrences matches {0..1} matches {    -- Relation to main tretament
												value matches {
													DV_CODED_TEXT matches {*}
												}
											}
											allow_archetype CLUSTER[at0017] occurrences matches {0..*} matches {    -- Additional details
												include
													archetype_id/value matches {/.*/}
											}
										}
									}
									allow_archetype CLUSTER[at0029] occurrences matches {0..*} matches {    -- Additional treatment
										include
											archetype_id/value matches {/.*/}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {    -- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
					CLUSTER[at0003] occurrences matches {0..1} matches {    -- Futher investigation
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0024] occurrences matches {0..1} matches {    -- Description futher investigation
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					ELEMENT[at0027] occurrences matches {0..1} matches {    -- Follow up
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0026] matches {*}    -- Item tree
		}
	}

