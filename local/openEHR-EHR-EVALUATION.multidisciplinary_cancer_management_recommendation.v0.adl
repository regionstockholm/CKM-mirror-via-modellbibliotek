		["en"] = <
			items = <
				["at0000"] = <
					text = <"multidisciplinary cancer management recommendation">
					description = <"The recommendation provided by a multidisciplinary team regarding the management of a cancer patient.">
				>
				["at0001"] = <
					text = <"Item tree">
					description = <"@ internal @">
