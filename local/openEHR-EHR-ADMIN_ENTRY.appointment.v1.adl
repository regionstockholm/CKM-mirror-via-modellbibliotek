		["sv"] = <
			items = <
				["at0000"] = <
					text = <"Bokning">
					description = <"En bokning av ett hälso- och sjukvårdsbesök mellan patient(er), hälso- och sjukvårdpersonal, närstående och/eller andra resurser tex instrument eller rum för ett specifikt datum/tid. Nära relaterat till FHIR Appointment-resurs.">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Status">
					description = <"Bokningens status">
				>
				["at0010"] = <
					text = <"Typ">
					description = <"Typen av möte (vanligtvis specialitet eller typ av möte).">
					comment = <"I FHIR definieras detta som \"Detta är koden som representerar den kliniska specialiteten hos klinikern eller vårdgivaren som interagerade med, behandlade eller tillhandahöll en tjänst till/för patienten. Kodverket som används för klinisk specialitet har begränsats av HITSP till kodverket reproducerad från HITSP C80 Tabell 2-149 Klinisk specialitet Värdemängddefinition.\" men kan vara ogiltig i jurisdiktioner utanför USA, och är därför lämnad oförändrad i denna arketyp.">
				>
				["at0011"] = <
					text = <"Orsak">
					description = <"Orsak till att mötet är bokat, \"Besöksorsak\".">
				>
				["at0012"] = <
					text = <"Prioritet">
					description = <"Prioritetsnivå för denna bokning">
					comment = <"Används för att fatta välgrundade beslut vid behov av omprioritering">
				>
				["at0013"] = <
					text = <"Beskrivning">
					description = <"En övergripande beskrivning av anledningen till bokningen">
					comment = <"Så som det kan visas på ämnesraden i en väntelista eller bokningslista">
				>
				["at0014"] = <
					text = <"Starttid">
					description = <"Starttid för bokningen/mötet">
				>
				["at0015"] = <
					text = <"Sluttid">
					description = <"Sluttid för bokningen/mötet">
				>
				["at0016"] = <
					text = <"Varaktighet">
					description = <"Den faktiska eller förväntade tidslängden på mötet.">
				>
				["at0017"] = <
					text = <"Kommentar">
					description = <"Ytterligare kommentarer">
				>
				["at0018"] = <
					text = <"Deltagare">
					description = <"*Details of intended participants, including patient, practitioners, devices or other resources. (en)">
				>
				["at0019"] = <
					text = <"Obligatorisk">
					description = <"Huruvida deltagaren är obligatorisk för mötet.">
					comment = <"(mötet handlar om dem, inte för dem - till exempel två läkare som diskuterar resultatet av en patients test).">
				>
				["at0020"] = <
