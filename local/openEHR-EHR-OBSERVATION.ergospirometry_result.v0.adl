					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Vo2max">
					description = <"The maximum amount of oxygen the body can uptake and utilize during intense physical exercise, serving as a key measure of aerobic fitness and cardiovascular endurance.">
				>
			>
		>
		["sv"] = <
			items = <
				["at0000"] = <
