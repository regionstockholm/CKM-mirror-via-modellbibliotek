							>
						>
					>
				}
			}
			ELEMENT[at0006] occurrences matches {0..1} matches {    -- External urethral sphincter invasion (EUSI)
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0035,    -- Very low suspicion of external urethral sphincter invasion
							at0036,    -- Low suspicion of external urethral sphincter invasion
							at0037,    -- Intermediate suspicion of external urethral sphincter invasion
							at0038,    -- Suspicion of external urethral sphincter invasion
							at0039]    -- Strong suspicion of external urethral sphincter invasion
						}
					}
				}
			}
			ELEMENT[at0007] occurrences matches {0..1} matches {    -- Bladderneck invasion (BI)
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0040,    -- Very low suspicion of bladderneck invasion
							at0041,    -- Low suspicion of bladderneck invasion
