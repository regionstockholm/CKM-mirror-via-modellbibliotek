definition
	CLUSTER[at0000] matches {    -- Detailed lesion findings prostate MRI
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0015] occurrences matches {0..1} matches {    -- Index lesion
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0016,    -- true
							at0017]    -- false
						}
					}
				}
			}
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- Extra-prostatic extension  (EPE)
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0020,    -- EPE very unlikely to be present
							at0021,    -- EPE unlikely to be present
							at0022,    -- Moderate probability that EPE is present
							at0023,    -- Suspected EPE
							at0024]    -- High probability that EPE is present
						}
					}
				}
			}
			ELEMENT[at0002] occurrences matches {0..1} matches {    -- Seminal vesicle invasion (SVI) left
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0025,    -- Very low suspicion of seminal vesicle invasion SVI
							at0026,    -- Low suspicion of seminal vesicle invasion SVI
							at0027,    -- Intermediate suspicion of seminal vesicle invasion SVI
							at0028,    -- Suspicion of seminal vesicle invasion SVI
							at0029]    -- Strong suspicion of seminal vesicle invasion SVI
						}
					}
				}
			}
			ELEMENT[at0003] occurrences matches {0..1} matches {    -- Seminal vesicle invasion  (SVI) right
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0030,    -- Very low suspicion of seminal vesicle invasion SVI
							at0031,    -- Low suspicion of seminal vesicle invasion SVI
							at0032,    -- Intermediate suspicion of seminal vesicle invasion SVI
							at0033,    -- Suspicion of seminal vesicle invasion SVI
							at0034]    -- Strong suspicion of seminal vesicle invasion SVI
						}
					}
				}
			}
			ELEMENT[at0004] occurrences matches {0..1} matches {    -- Ingrowth height left
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"mm">
							>
						>
					>
				}
			}
			ELEMENT[at0005] occurrences matches {0..1} matches {    -- Ingrowth height right
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
