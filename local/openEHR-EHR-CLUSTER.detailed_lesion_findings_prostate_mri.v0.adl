							["1"] = <
								units = <"mm">
							>
						>
					>
				}
			}
			ELEMENT[at0011] occurrences matches {0..1} matches {    -- PI-RADS score T2W TZ
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0056,    -- Homogeneous intermediate signal intensity (normal)
							at0057,    -- Circumscribed hypointense or heterogeneous encapsulated nodule(s) (BPH)
							at0058,    -- Heterogeneous signal intensity with hidden margins (includes others not designated as 2, 4 or 5)
							at0059,    -- Lenticular or non-circumscribed, homogeneous, moderately hypointense and <1.5 cm in greatest dimension
							at0060]    -- Lenticular or non-circumscribed, homogeneous, moderately hypointense and >=1.5 cm in greatest dimension or definite extraprostatic extension/invasive behaviour
						}
					}
				}
			}
			ELEMENT[at0055] occurrences matches {0..1} matches {    -- PI-RADS score T2W PZ
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0061,    -- Uniform hyperintense signal intensity (normal)
							at0062,    -- Linear or wedge-shaped hypointensity or diffuse mild hypointensity, usually indistinct margin
							at0063,    -- Heterogeneous signal intensity or non-rewritten, rounded, moderate hypointensity (includes others not labelled as 2, 4 or 5)
							at0064,    -- Rewritten, homogeneous moderate hypointensity focus/mass confined to the prostate and <1.5 cm in greatest dimension
							at0065]    -- Circumscribed, homogeneous moderate hypointensity focus/mass confined to the prostate and >=1.5 cm in greatest dimension or definite extraprostatic extension/invasive behaviour
						}
					}
				}
			}
			ELEMENT[at0012] occurrences matches {0..1} matches {    -- PI-RADS score DWI/ADC
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0066,    -- No abnormality (i.e. normal) on ADC and high b-value DWI
							at0067,    -- Not distinct hypointense on ADC
							at0068,    -- Focal mild/moderate hypointense on ADC and isointense/mild hyperintense on high b-value DWI
							at0069,    -- Focal marked hypointense on ADC and marked hyperintense on high b-value DWI; <1.5 cm in largest dimension
							at0070]    -- Focal marked hypointense on ADC and marked hyperintense on high b-value DWI; >=1.5 cm in largest dimension or definite extraprostatic extension/invasive
						}
					}
				}
			}
			ELEMENT[at0014] occurrences matches {0..1} matches {    -- Dynamic contrast enhancement (DCE)
				value matches {
					DV_CODED_TEXT matches {*}
				}
			}
		}
	}

