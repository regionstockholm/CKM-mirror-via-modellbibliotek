definition
	CLUSTER[at0000] matches {    -- Microscopy findings summary
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0210] occurrences matches {0..1} matches {    -- Number of invasive tumours
				value matches {
					DV_COUNT matches {*}
					DV_CODED_TEXT matches {*}
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0246] occurrences matches {0..1} matches {    -- Assessability of number of invasive tumours
				value matches {
					DV_CODED_TEXT matches {*}
				}
			}
			ELEMENT[at0227] occurrences matches {0..1} matches {    -- Comment on number of invasive tumours
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0218] occurrences matches {0..1} matches {    -- Size of largest invasive tumour
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"mm">
							>
						>
					>
				}
			}
			CLUSTER[at0196] occurrences matches {0..1} matches {    -- Distribution
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0207] occurrences matches {0..1} matches {    -- Assessability of total lesion extent
						value matches {
							DV_CODED_TEXT matches {*}
							DV_BOOLEAN matches {*}
						}
					}
					ELEMENT[at0194] occurrences matches {0..1} matches {    -- Total lesion extent
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::122]>
								list = <
									["1"] = <
										units = <"mm">
									>
								>
							>
						}
					}
					ELEMENT[at0198] occurrences matches {0..1} matches {    -- Comment on total lesion extent
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0225] occurrences matches {0..1} matches {    -- Focality
						value matches {
							DV_CODED_TEXT matches {*}
						}
					}
					ELEMENT[at0226] occurrences matches {0..1} matches {    -- Comment on focality
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
			allow_archetype CLUSTER[at0248] occurrences matches {0..*} matches {    -- Surgical resection margins
				include
					archetype_id/value matches {/.*/}
			}
			allow_archetype CLUSTER[at0209] occurrences matches {0..*} matches {    -- Additional details
				include
					archetype_id/value matches {/.*/}
			}
			ELEMENT[at0245] occurrences matches {0..1} matches {    -- Comment on microscopy findings summary
				value matches {
					DV_TEXT matches {*}
				}
			}
		}
	}

