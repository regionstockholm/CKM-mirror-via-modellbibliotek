definition
	CLUSTER[at0000] matches {    -- Medication trial
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0005] occurrences matches {0..1} matches {    -- Clinical trial medication?
				value matches {
					DV_BOOLEAN matches {*}
				}
			}
			ELEMENT[at0034] occurrences matches {0..1} matches {    -- Trial name
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0032] occurrences matches {0..1} matches {    -- Blinded medication name/code
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0033] occurrences matches {0..1} matches {    -- Unblinding result
				value matches {
					DV_TEXT matches {*}
				}
			}
		}
	}

